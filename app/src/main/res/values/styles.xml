<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools"
    >
    <style name="toolbar_title">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
    </style>
    <style name="home_welcome_text_default">
        <item name="android:textSize">24sp</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="fontFamily">@font/roboto_medium</item>
    </style>
    <style name="home_welcome_text_userDynamic">
        <item name="android:textSize">24sp</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:textColor">#8CB1FF</item>
        <item name="fontFamily">@font/roboto_medium</item>
    </style>
    <style name="home_current_time">
        <item name="android:textSize">50sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#D1FFFFFF</item>
        <item name="fontFamily">@font/roboto_medium</item>
    </style>
    <style name="home_current_date">
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#918DA0</item>
        <item name="fontFamily">@font/roboto_medium</item>
    </style>
    <style name="home_stats_card_head">
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="fontFamily">@font/roboto_medium</item>
    </style>
    <style name="home_stats_card_value">
        <item name="android:textSize">34sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="fontFamily">@font/roboto_medium</item>
    </style>
    <style name="calendar_month_text">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#CAC4D0</item>
    </style>
    <style name="calendar_week_day_text">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">0.03</item>
        <item name="android:textColor">#E6E0E9</item>
    </style>
    <style name="CustomAlertDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <!-- Customize dialog background color -->
        <item name="android:background">#1C2A4F</item>
        <!-- Customize title text color -->
        <item name="android:textColorPrimary">#EB3B81FD</item>
        <!-- Customize message text color -->
        <item name="android:textColorSecondary">#EBFFFFFF</item>
        <!-- Customize button text color -->
        <item name="colorAccent">#D0FFFFFF</item>
    </style>

    <style name="leave_card_header">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">24sp</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="leave_card_duration">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">11sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#B3FFFFFF</item>
    </style>
    <style name="leave_card_count">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">17sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="leave_card_status_text">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">12sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="new_leave_header">
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="new_leave_txt">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#CCFFFFFF</item>
    </style>
    <style name="attendance_percentage_text">
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textSize">40sp</item>
        <item name="android:textColor">#8381FF</item>
    </style>
    <style name="dialog_leave_delete_warning">
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="dialog_leave_delete_warning_txt">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="Theme.DoNotDim" parent="android:Theme">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <style name="profile_fields_key">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#C4FFFFFF</item>
    </style>
    <style name="ImageView.Circle" parent="">
        <item name="cornerSize">50%</item>
    </style>

</resources>